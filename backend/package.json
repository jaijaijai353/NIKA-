{
  "name": "nika-backend",
  "version": "1.0.0",
  "description": "NIKA Backend API for dataset processing",
  "main": "server.ts",
  "scripts": {
    "start": "npx ts-node server.ts",
    "dev": "nodemon --watch './**/*.ts' --exec 'node --loader ts-node/esm' server.ts",
    "build": "npx tsc",
    "clean": "rm -rf dist uploads/*.* db.sqlite",
    "test": "echo \"Error: no test specified\" && exit 1"
